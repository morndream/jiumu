//绝对定位居中
.abs_middle{
  	width: 100%;
    position: absolute;
    top 0
    left 0
    // top 40%
    // transform: translateY(-50%)
}
//固定定位全屏
.fixed{
  position fixed
  top 50%
  left 50%
  transform: translate(-50%,-50%)
}
.abs_bt{
  position absolute
  // bottom 8px
  bottom 0
  left 0
  z-index 9;
  // left 50%
  // transform: translate(-50%,-50%)  
}
.abs_bt2{
  position absolute !important
  // bottom 8px
  bottom 0
  left 0
  z-index 9;
}
.abs_bt3{
  position absolute !important
  // bottom 8px
  bottom 0
  left 0
  z-index -1;
}
//固定定位居中
.fix_middle{
  	width: 100%;
    position: fixed;
    top 0
    left 0
}


