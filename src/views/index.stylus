.showbox{
   	width:100%;
   	height: 96%;
    margin 0 auto
  	background-color: #101010; 
    .content{
      width 78%
      margin 0 auto
      .row{
        color #fff  
        .col{
          padding 30px 0
          border-bottom 0.1px solid #888
        }
        div:first-child{
          width 28%
          margin-right 50px
        }
        div:last-child{
          width 62%
        }
      }
      .t1{
        font-size 50px
      }
      .t2{
        div:first-child{
          font-size:36px
        }
        div:last-child{
         font-size: 24px;
        }
      }
      .t3{
        color #aaa
        margin 21px 0
      }
      .t4{
        color #fff
        font-size: 30px;
        width 635px
        height 50px;
        line-height 50px
        margin 30px auto
        border 1px solid #888 
        border-radius 50px
        img{
          width 32px
          margin-right 10px
        }
      }
      .row2{
        color #fff  
        .col{
          padding 44px 0
          border-bottom 0.1px solid #888
        }
        .c1{
          width 40%
          margin-right 50px
        }
        .c2{
          width 50%
        }          
      }
      .right{
        width 182px
        height 150px 
        div{
          height 30px
          line-height 30px
          font-size: 21px;
          color: #aaaaaa;
          
        }
        .line{
          width 67px
          height 15px
          margin-right 10px
          border-bottom 1px dotted #aaa 
        }
        .right-bottom{
          margin-bottom:10px  
        }
      }
      .t5{
        font-size: 24px;
        color #aaa
        div:first-child{
          color #fff
        }      
      }
    }
    .content2{
      color #fff
      padding-top:60px
      .row{
        width 733px
        margin 0 auto
      }
      .r2{
        margin-top 50px  
      }
      .imgbox{
         width 64px
         img{
           width 100%
         }
      }
      .t1{
         font-size: 50px;
         padding 45px 0
         border-bottom 1px solid #aaa
      }
      .t2{
       font-size: 24px;
       letter-spacing: 0px;
       padding 41px 0 38px
      }
      .t3{
       font-size: 24px;
       color #aaa  
       padding-bottom 50px
      }
    }
}
