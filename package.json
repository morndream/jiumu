{
  "name": "jomoo",
  "version": "1.0.0",
  "description": "Collection of vant demos.",
  "author": "Chenzf <372624240@qq.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "amfe-flexible": "^2.2.1",
    "autoprefixer": "latest",
    "axios": "^0.21.1",
    "vant": "^2.1.8",
    "vue": "^2.6.10",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.0.6",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.3",
    "babel-plugin-import": "^1.12.1",
    "postcss-pxtorem": "^4.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "indent": [
        1,
        2
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 5 versions",
          "Android >= 4.0",
          "not ie <= 8"
        ]
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "ios >= 8",
    "android >= 4.0"
  ],
  "license": "MIT",
  "main": "babel.config.js",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/morndream/template.git"
  }
}
